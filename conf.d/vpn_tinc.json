{
	"name": "VPN Tinc Server",
	"type": "container",
	"description": "VPN server built on top of Tinc",
	"kind": "vpn",
	"flavor": "tinc",
	"image": "jenserat/tinc",
	"net": "host",
	"device": "/dev/net/tun",
	"capabilities": ["NET_ADMIN"],
	"volumes": ["/mnt/sda1/var/data/vpn:/etc/tinc"],
	"command": ["start", "-D"]
}